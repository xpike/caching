{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "XPike": {
    "Logging": {
      "Console": {
        "ConsoleLogConfig": {
          "Enabled": "true",
          "ShowMetadata": "false"
        }
      },
      "Debug": {
        "DebugLogConfig": {
          "Enabled": "true",
          "ShowMetadata": "true"
        }
      }
    },
    "Caching": {
      "Redis": {
        "RedisCachingConfig": {
          "Connections": {
            "default": {
              "RedisConnectionName": "default",
              "Enabled": "true",
              "DefaultGetTimeoutMs": "5000",
              "DefaultSetTimeoutMs": "5000"
            }
          }
        }
      }
    },
    "Redis": {
      "RedisConfig": {
        "Connections": {
          "default": {
            "ConnectionString": "127.0.0.1:6379",
            "Enabled": "true"
          }
        }
      }
    }
  }
}
